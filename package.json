{
  "name": "@fluido/ui-containers",
  "version": "0.5.4-beta.19",
  "license": "MIT",
  "private": false,
  "description": "A framework for create front-end easy and fast",
  "keywords": [
    "fluido",
    "front-end",
    "styled",
    "styled-components",
    "goober",
    "chakra",
    "react",
    "framework",
    "layout"
  ],
  "author": {
    "email": "morbden.dev@gmail.com",
    "name": "Morbden"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Morbden/fluido-ui-containers"
  },
  "main": "dist/index.js",
  "module": "esm/index.js",
  "scripts": {
    "dev": "run-p dev:common dev:esm",
    "dev:common": "tsc -w",
    "dev:esm": "tsc --build tsconfig.esm.json -w",
    "build": "run-s clear build:common build:esm",
    "clear": "rimraf dist esm",
    "build:common": "tsc",
    "build:esm": "tsc --build tsconfig.esm.json",
    "pub": "npm publish --access public",
    "pub:beta": "npm publish --access public --tag beta",
    "pub:alpha": "npm publish --access public --tag alpha",
    "pub:all": "run-s build pub",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "@stitches/react": "^1.x.x",
    "react": ">=16.8.0 <=17.x.x",
    "react-dom": ">=16.8.0 <=17.x.x"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@stitches/react": "^1.2.1",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/react": "^6.3.8",
    "@types/node": "^16.10.1",
    "@types/react": "^17.0.24",
    "babel-loader": "^8.2.2",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.3"
  }
}
